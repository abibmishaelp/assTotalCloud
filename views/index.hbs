<h1>{{title}}</h1>
<p>Welcome to {{subTitle}}</p>
<link rel='stylesheet' href='/stylesheets/style.css' />

<body>  
    {{#if users}}
    {{#each users}}
    <div class="gallery">
        <a target="_self" href="/getDetails/{{this.id}}" action = 'get' >
            <img src={{this.avatar}} alt={{this.avatar}} width="60" height="40" value = {{this.id}}>
        </a>
        {{!-- <p> {{this.first_name}} {{this.last_name}} </p> --}}
        <label value={{this.id}} name="userId">{{this.first_name}} {{this.last_name}}</label>
    </div>
    {{/each}}
    {{/if}}
</body>